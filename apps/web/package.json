{
  "name": "web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "clean": "git clean -xdf .next .turbo node_modules",
    "dev": "next dev",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint .",
    "start": "next start",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "@trroev/prettier-config",
  "eslintConfig": {
    "extends": [
      "@trroev/eslint-config/base",
      "@trroev/eslint-config/nextjs",
      "@trroev/eslint-config/react"
    ],
    "ignorePatterns": [
      "next-env.d.ts"
    ],
    "root": true
  },
  "dependencies": {
    "@trroev/layout": "workspace:*",
    "@trroev/page": "workspace:*",
    "@trroev/ui": "workspace:*",
    "next": "catalog:next14",
    "react": "catalog:next14",
    "react-dom": "catalog:next14"
  },
  "devDependencies": {
    "@trroev/eslint-config": "workspace:*",
    "@trroev/tailwind-config": "workspace:*",
    "@trroev/tsconfig": "workspace:*",
    "@types/node": "^20.14.15",
    "@types/react": "catalog:next14or15",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.6.2"
  }
}
